<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="340.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory_project.FXMLAddPartController">
   <children>
      <VBox prefHeight="360.0" prefWidth="311.0" spacing="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="14.0" prefWidth="291.0">
               <padding>
                  <Insets right="10.0" />
               </padding>
               <opaqueInsets>
                  <Insets left="10.0" right="10.0" />
               </opaqueInsets>
            </HBox>
            <VBox prefHeight="272.0" prefWidth="291.0">
               <children>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="75.0" text="Type" />
                        <Group>
                           <children>
                              <RadioButton fx:id="inHouseRadioButton" mnemonicParsing="false" prefHeight="23.0" prefWidth="126.0" selected="true" text="InHouse">
                                 <padding>
                                    <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                 </padding>
                              </RadioButton>
                              <RadioButton fx:id="outSourcedRadioButton" layoutX="75.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="122.0" text="OutSourced">
                                 <padding>
                                    <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                                 </padding>
                              </RadioButton>
                           </children>
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                           <rotationAxis>
                              <Point3D />
                           </rotationAxis>
                        </Group>
                     </children>
                  </HBox>
                  <HBox prefHeight="34.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="ID" />
                        <TextField fx:id="idTextfield" promptText="Part ID" />
                     </children>
                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="Name" />
                        <TextField fx:id="nameTextfield" promptText="Part Name" />
                     </children>
                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="Qty" />
                        <TextField fx:id="qtyTextfield" promptText="Inventory Qty." />
                     </children>
                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="Price" />
                        <TextField fx:id="priceTextfield" promptText="Price" />
                     </children>
                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="Max" textAlignment="RIGHT" />
                        <TextField fx:id="maxTextfield" prefHeight="27.0" prefWidth="55.0" promptText="max" />
                        <Label prefHeight="17.0" prefWidth="27.0" />
                        <Label prefHeight="17.0" prefWidth="29.0" text="Min" />
                        <TextField fx:id="minTextfield" prefHeight="27.0" prefWidth="55.0" promptText="min" />
                     </children>
                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label disable="true" prefWidth="100.0" text="Company" />
                        <TextField fx:id="companyTextfield" disable="true" promptText="Company" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </VBox>
            <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="55.0" prefWidth="189.0" spacing="10.0">
               <children>
                  <Button fx:id="cancelPartButton" cancelButton="true" contentDisplay="CENTER" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT" onAction="#cancelPartButtonClicked" prefHeight="27.0" prefWidth="74.0" text="Cancel" />
                  <Button fx:id="savePartButton" contentDisplay="CENTER" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT" onAction="#savePartButtonClicked" prefHeight="27.0" prefWidth="75.0" text="Save" />
                  <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetToDefault" text="Reset" textFill="#113eae">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
